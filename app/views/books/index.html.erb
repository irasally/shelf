<h1>Book List</h1><%= form_tag [:search, :books],    :method => :get,    :style => "text-align: right" do %><%= text_field_tag "query", params[:query] %><%= submit_tag "タイトル検索" %><% end %><ul class="navigation"><% if params[:action] == "index" %><li class="selected">在庫</li><% else %><li><%= link_to "在庫", :books %></li><% end %><% if params[:action] == "checked_out" %><li class="selected">貸し出し中</li><% else %><li><%= link_to "貸し出し中", [:checked_out, :books] %></li><% end %></ul><table class="books">  <col class="title"/>  <col class="authers"/>  <col class="publish_year"/>  <th>Title</th>  <th>Authers</th>  <th>Publish Year</th>  <th></th>  <% @books.each do |book| %>  <tr>    <td><%= link_to book.title, book %></td>    <td><%= book.authers %></td>    <td><%= book.publish_year %></td>    <td><%= link_for(book)%></td>  </tr>  <% end %></table><%= will_paginate @books,    :previous_label => '<-前', :next_label => '次->' ,    :inner_window => 2, :outer_window => 2%><p class="link_to"><%= link_to "新規作成", [:new, :book ] %></p>